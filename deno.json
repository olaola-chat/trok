{
  "name": "@trok/trok",
  "version": "0.1.61",
  "license": "MIT",
  "exports": {
    "./cli": "./lib/cli.ts",
    "./workspace": "./server/workspace.tsx",
    "./hub": "./server/hub.tsx",
    "./aliflow": "./server/aliflow.tsx",
    "./summaryDist": "./lib/summaryDist.ts"
  },
  "imports": {
    "@std/cli": "jsr:@std/cli@^1.0.9",
    "@std/path": "jsr:@std/path@^1.0.8",
    "preact": "npm:preact@10.25.4",
    "preact-render-to-string": "npm:preact-render-to-string@^6.5.12"
  },

  "tasks": {
    "serve:workspace": "deno serve --watch -A --host 127.0.0.1 server/workspace.tsx",
    "serve:hub": "deno serve --watch -A --host 127.0.0.1 server/hub.tsx",
    "test:cli": "deno -A lib/cli.ts --notify https://api.sleeplessplanet.com,https://github.com/asdf/80asdf",
    "buildUI": "deno -A buildUI.ts"
  },

  "fmt": {
    "lineWidth": 80,
    "indentWidth": 2
  }
}
